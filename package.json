{
  "name": "meme-view",
  "version": "2.1.0",
  "description": "custom view for meme.",
  "repository": "fuchunhui/meme-view",
  "main": "dist/imeme.umd.js",
  "module": "dist/imeme.es.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build --mode development",
    "type-check": "vue-tsc --build",
    "lib": "vue-tsc --build && vite build --mode lib --config vite.lib.config.ts",
    "serve": "vite preview",
    "deploy": "",
    "release": "pnpm run lib && node scripts/release.js",
    "commitlint": "node scripts/verify-commit.js"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "typescript": "~5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vue-tsc": "^2.1.10",
    "chalk": "^5.3.0",
    "enquirer": "^2.4.1",
    "execa": "^9.5.2",
    "husky": "^8.0.3",
    "less": "^4.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "semver": "^7.6.3"
  },
  "files": [
    "dist",
    "README.md",
    "index.d.ts"
  ],
  "keywords": [
    "imeme",
    "meme",
    "vite",
    "lib",
    "robot"
  ],
  "author": "fuchunhui",
  "bugs": {},
  "homepage": "",
  "engines": {
    "node": ">=16.0"
  }
}
